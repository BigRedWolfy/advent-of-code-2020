git -> https://www.youtube.com/watch?v=SWYqp7iY_Tc&t=689s

git init 

git config --global user.name 'James Feldmayer'
git config --global user.email 'jamesfeldmayer01@gmail.com'

//staging (prior to commit)
git add main.cpp  
git rm main.cpp

git status 

//commit changes
git commit //vim
git commit -m 'comment' //explicit insertion

touch .gitignore
contents: log.txt
          *.txt
          folder2

touch log.txt
contents: some error messages

git branch example //create branch 'example'
git checkout example //move to branch 'example'

git merge example -m 'added example'

touch         //working directory
-> git add    //staging area
-> git commit //repository

git remote rm origin

git remote add origin https://github.com/DrNoobopolis/csit321-project.git
git remote 
